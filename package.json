		"@tremor/react": "^3.2.6",
		"@types/node": "20.3.2",
		"@types/react": "18.2.14",
		"@types/react-dom": "18.2.6",
		"autoprefixer": "10.4.14",
		"cal-sans": "^1.0.1",
		"class-variance-authority": "^0.6.1",
		"clsx": "^1.2.1",
